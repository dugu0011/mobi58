                                    <th scope="col">Total Amount</th>
                                    <th scope="col">Pending Amount</th>
                                </tr>
                            </thead>
                            <tbody class="text-center">  
                                
                                {% for m in monthly_data %}
                                <tr>
                                <td>{{m.in_date}}</td>
                                <td>{{m.net_weight}}</td>
                                <td>{{m.gst_amt}}</td>
                                <td>{{m.grand_total}}</td>
                                <td>{{m.pending_amt}}</td>
                                </tr>
                                {% endfor %}
                                
                            </tbody>
